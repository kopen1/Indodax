{

 "functions": {
    "scalping/api/index.py": {
      "runtime": "python3.11"
    }
  "rewrites": [
    { "source": "/scalping", "destination": "/scalping/api/index.py" }
  ],
  "builds": [
    { "src": "app.py", "use": "@vercel/python"}
  ],
  "routes": [
    { "src": "/(.*)", "dest": "app.py"}
  ]
}
